<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="css/styles.css">
  <title>Document</title>
</head>

<body>
  <div>
    <form action="" method="post"><input type="submit"></form>
  </div>
  <% etiquetas.forEach(function(et){ %>
    <div class="super-container">

      <div class="container">
        <div>Oferta</div>
        <div>
          <%= et.BARCODE %>
        </div>
        <div>11509</div>
        <div>$</div>
        <div>
          <%= (et.PUNIT).toString().split('.')[0] %>
            <sup
              style="text-decoration: underline;font-size: 18px;margin: 2px;"><%= (et.PUNIT).toString().split('.')[1].Number %></sup>
        </div>
        <div>
          <%= et.DESCRIP %>
        </div>
      </div>
      <div class="container">
        <div>Oferta</div>
        <div>
          <%= et.BARCODE %>
        </div>
        <div>11509</div>
        <div>$</div>
        <div>
          <%= et.PUNIT %><sup style="text-decoration: underline;font-size: 18px;margin: 5px;">00</sup>
        </div>
        <div>
          <%= et.DESCRIP %>
        </div>
      </div>

    </div>
    <% }); %>
</body>

</html>